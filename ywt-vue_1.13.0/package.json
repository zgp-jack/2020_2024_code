{
  "name": "ywt-vue",
  "version": "1.13.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:prod": "vue-cli-service build",
    "build:stage": "vue-cli-service build --mode staging",
    "build:hxfe": "vue-cli-service build --mode hxfe",
    "report": "vue-cli-service build --report",
    "lint": "eslint --ext .js,.vue src --fix",
    "commit": "git-cz",
    "prepare": "husky install",
    "doc": "jsdoc.cmd -c jsdoc.json -R document.md && cd docs && http-server"
  },
  "dependencies": {
    "@hyjiacan/vue-ellipsis": "^1.1.6",
    "axios": "^0.21.1",
    "bignumber.js": "^9.1.1",
    "blueimp-md5": "^2.19.0",
    "core-js": "^3.6.5",
    "crypto-js": "^4.2.0",
    "dhtmlx-gantt": "^6.3.7",
    "echarts": "^5.3.1",
    "element-ui": "^2.15.5",
    "flat": "^5.0.2",
    "fuse.js": "^3.4.4",
    "js-base64": "^3.7.2",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^2.4.0",
    "pinyin": "^2.10.2",
    "print-js": "^1.6.0",
    "screenfull": "^4.2.0",
    "sortablejs": "^1.13.0",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.3.3",
    "vue-grid-layout": "^2.3.12",
    "vue-image-crop-upload": "^3.0.3",
    "vue-router": "^3.5.1",
    "vue-virtual-scroll-list": "^2.3.4",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.2",
    "wangeditor": "^4.7.10",
    "wl-core": "^1.1.9"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "clean-jsdoc-theme": "^4.2.2",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "filemanager-webpack-plugin": "^4.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "postcss-px-to-viewport": "^1.1.1",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "sass-resources-loader": "^2.2.4",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "svg-sprite-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
