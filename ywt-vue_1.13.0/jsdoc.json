{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
      "include": ["src/common/utils"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
  },
  "sourceType": "module",
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
},
  "opts": {
      "recurse": true,
      "destination": "./docs/",
      "theme_opts": {
        "default_theme": "dark"
      },
      "template": "node_modules/clean-jsdoc-theme"
  }
}
